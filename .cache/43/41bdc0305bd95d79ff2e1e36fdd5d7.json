-left: -100%;\n  transition: margin 0.3s ease-in 0s;\n}\n.onoffswitch-inner:before, .onoffswitch-inner:after {\n    display: block; float: left; width: 50%; height: 24px; padding: 0; line-height: 24px;\n    font-size: 14px; color: white; font-family: Trebuchet, Arial, sans-serif; font-weight: bold;\n    box-sizing: border-box;\n}\n.onoffswitch-inner:before {\n    content: \"\";\n    padding-left: 10px;\n    background-image: linear-gradient(to right,#378fe6, #3eda82);\n}\n.onoffswitch-inner:after {\n    content: \"\";\n    padding-right: 10px;\n    background-image: linear-gradient(to right,#378fe6, #3eda82);\n    text-align: right;\n}\n.onoffswitch-switch {\n    display: block; width: 20px; margin: 2px;\n    background: #FFFFFF;\n    position: absolute; top: 0; bottom: 0;\n    right: 26px;\n    border-radius: 20px;\n    transition: all 0.3s ease-in 0s; \n}\n.onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-inner {\n    margin-left: 0;\n}\n.onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-switch {\n    right: 0px; \n}\n\n\n/* TOP ROW CONTAINER */\n\n.top-row-grid {\n  width: 100%;\n  margin: 0 auto;\n  clear: both;\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.top-row-box {\n  background-color: hsl(227, 47%, 96%);\n  text-align: center;\n  padding: 2em;\n  margin: 2em 0;\n  border-radius: 2%;\n}\n\n.top-row-box:hover  {\n  background-color: hsl(230, 22%, 74%);\n}\n\n.inner-body-cheat:hover {\n  background-color: hsl(230, 22%, 74%);\n}\n\n.inner-box-top-row {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.facebook-panel-top {\n  border-top: .25em solid hsl(208, 92%, 53%);\n}\n\n.twitter-panel-top {\n  border-top: .25em solid hsl(203, 89%, 53%);\n}\n\n.thegram-panel-top {\n  border-top: .25em solid transparent;\n  /* border-image-source: \n  border-image-slice: 1; */\n  /* background:  linear-gradient(var(--thegram-background-color), 2%, hsl(227, 47%, 96%), 2% ); */\n  background:  linear-gradient(to right, hsl(37, 97%, 70%) 0%, hsl(329, 70%, 58%));\n  padding: 0;\n}\n\n.inner-body-cheat {\n  background-color: hsl(227, 47%, 96%);\n  /* width: 100%;\n  height: 100%; */\n  text-align: center;\n  padding: 2em;\n  /* border-radius: 2%; */\n  border-radius: 0 0 2% 2%;\n}\n\n\n.youtube-panel-top {\n  border-top: .25em solid hsl(348, 97%, 39%);\n}\n\n.top-bar-social {\n  font-weight: 700;\n  font-size: .8em;\n  display: inline-block;\n  margin-left: .5em;\n  color: hsl(230, 17%, 14%);\n}\n\n.social-logo {\n  display: inline-block;\n}\n\n.top-bar-count {\n  font-weight: 700;\n  font-size: 4em;\n  margin-top: 0.2em;\n  color: hsl(230, 17%, 14%);\n}\n\n\n.top-bar-followers {\n  text-transform: uppercase;\n  letter-spacing: .4rem;\n  font-size: .8rem;\n  color: hsl(228, 12%, 44%);\n}\n\n.inner-box-bottom-row {\n  margin-top: 1.1em;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.top-bar-today-count {\n  font-size: .8rem;\n  display: inline-block;\n  font-weight: 700;\n}\n\n\n/* BOTTOM GRID */\n\n.overview-heading {\n  margin-bottom: 1em;\n  color: hsl(228, 12%, 44%);\n  font-size: 1.2rem;\n}\n\n.bottom-row-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.bottom-row-box {\n  background-color: hsl(227, 47%, 96%);\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  grid-template-rows: 1fr 1fr;\n  padding:  1.6em 2em;\n  border-radius: 2%;\n}\n\n.bottom-row-box:hover  {\n  background-color: hsl(230, 22%, 74%);\n}\n\n\n.bottom-grid-item-top {\n  margin-bottom: 1em;\n}\n\n.page-views {\n  font-size: .9em;\n  font-weight: 700;\n  color: hsl(228, 12%, 44%)\n}\n\n.social-img-bottom {\n  justify-self: end;\n}\n\n.bottom-row-count-number {\n  font-size: 2em;\n  color: hsl(230, 17%, 14%);\n  font-weight: 700;\n  margin-top: 0;\n}\n\n.percent-change-bottom {\n  align-self: end;\n  justify-self: end;\n}\n\n/* FOOTER */\n\nfooter {\n  margin-top: 2rem;\n  text-align: center;\n  margin: 0 auto;\n}\n\n.attribution {\n  margin-top: 2em;\n  font-size: 11px;\n  text-align: center;\n}\n\n.attribution a {\n  color: hsl(228, 45%, 44%);\n}\n\n.headingNumbers {\n  color: hsl(0, 0%, 100%);\n}\n\n.darkModeCard {\n  background-color: hsl(228, 28%, 20%);\n  color: hsl(228, 34%, 66%);\n}\n\n.darkModeCard:hover {\n  background-color:hsla(228, 34%, 66%, .3);\n}\n\n\n/*\n- Very Dark Blue (BG): hsl(230, 17%, 14%)\n- Very Dark Blue (Top BG Pattern): hsl(232, 19%, 15%)\n- Dark Desaturated Blue (Card BG): hsl(228, 28%, 20%)\n- Desaturated Blue (Text): hsl(228, 34%, 66%)\n- White (Text): hsl(0, 0%, 100%)\n*/\n\n.htmlDark {\n  background: hsl(230, 17%, 14%);\n}\n\n.bodyDark {\n  background: linear-gradient(hsl(232, 19%, 15%) 35%, hsl(230, 17%, 14%) 35%);\n}\n\n.socialMediaHeadingsDark {\n  color: hsl(228, 12%, 44%);\n}\n\n\n@media only screen and (max-width: 600px) {\n\n  * {\n    margin: 0;\n    padding: 0;\n    display: block;\n  }\n\n  .wrapper {\n    width: 95%;\n  }\n\n  .toggle-button-container {\n    width: 100%;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n  }\n\n  hr {\n    display: inherit;\n    color: black;\n    width: 100%;\n  }\n\n  .top-row-box {\n    margin-bottom: 1em;\n  }\n\n}"},"lineCount":375}},"error":null,"hash":"247b83da3bbae218d304de05b451c15f","cacheData":{"env":{}}}